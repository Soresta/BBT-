public class hafta4_3 {
    public static void main(String[] args) {
        //1-100 aras覺nda asal say覺lar覺 bulan program.
        for (int i = 2; i <= 100; i++) {
            boolean asal = true;
            for (int j = 2; j < i / 2; j++) {
                if (i % j == 0) {
                    asal = false;
                    break;
                }
            }
            if (asal) {
                System.out.println("Asal say覺 : " + i);
            }
        }
    }
}
