public class hafta4 {
    public static void main(String[] args) {
        //Ad ve soyadınızı 100 kere yazdıran programı yazınız!
        String fullName = "Ad  Soyad";
        int i = 1;
        while (i <= 100){
            System.out.println(i+ ". "+ fullName);
            i++;
        }
    }
}
